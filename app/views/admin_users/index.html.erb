<% @page_title = 'All Admin Users'%>
<%= link_to("<< Back to Menu", admin_pa)%>
<div class="users index">
  <h2>Admin Users</h2>
  <% image_tag('plus.png', :size => '11x11', :alt => '+')%>
  <% link_to("Add new User", '#', :class => 'action new')%>
  <table class="listing" summary="AdminUser Table">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>User Name</th>
      <th>Actions</th>
    </tr>
    <% for user in @admin_users %>
    <tr>
      <td><%= user.first_name%></td>
      <td><%= user.last_name%></td>
      <td><%= user.email%></td>
      <td><%= user.username%></td>
      <td class="actions">
          <%=link_to("Edit", '#', :class => 'action_edit') %>
          <%=link_to("Delete", '#', :class => 'action_delete') %>
      </td>
    </tr>
    <% end %>
  </table>

</div>
